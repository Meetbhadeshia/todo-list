<%- include("header") -%>

    <h1 class="box" id="heading"><%= listTitle %></h1>
    <div class="box">
      <% for (let i = 0 ; i < newListItems.length; i++) { %>
      <div class="item">
        <input type="checkbox" />
        <p><%= newListItems[i] %></p>
      </div>
      <% } %>
      <form class="item" action="/" method="post">
        <input
          type="text"
          name="todoItem"
          placeholder="Type here"
          autocomplete="off"
        />
        <button type="submit" name="list" value=<%= listTitle %>>+</button>
      </form>
    </div>

<%- include("footer") -%>